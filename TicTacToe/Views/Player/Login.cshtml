@{
}
@model TicTacToe.Models.Player;

<form action="Login" method="post">

    <fieldset>
        <label id="PlayerEmail">Email</label>
        <input type="email" required name="PlayerEmail" value="@Model?.PlayerEmail" />
    </fieldset>


    <fieldset>
        <label id="PlayerPassword">Password</label>
        <input type="password" required name="PlayerPassword" value="@Model?.PlayerPassword" />
    </fieldset>

   
    <button type="submit">Login</button>
</form>
